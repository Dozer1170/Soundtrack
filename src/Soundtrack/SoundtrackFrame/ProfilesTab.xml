<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="SoundtrackFrameProfilesFrameTemplate" hidden="true" virtual="true">
        <Size>
            <RelDimension x="0.5" y="0.8"/>
        </Size>
        <Anchors>
            <Anchor point="TOP" relativeTo="SoundtrackFrame" relativePoint="TOP"/>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <FontString name="SoundtrackFrame_FontStringUIOptions" inherits="GameFontNormal" text="Profiles">
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset>
                                <AbsDimension x="50" y="-50"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
        </Frames>
        <Scripts>
            <OnShow>SoundtrackFrame_RefreshProfilesFrame()</OnShow>
        </Scripts>
    </Frame>
</Ui>